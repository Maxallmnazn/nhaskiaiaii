let platnomor = ["B 444 JGN"]
let tanggal= 5
let denis = platnomor[0].split(" ")
let tanggalganjil = denis%2===1 && tanggal %2===1
let tanggalgenap = denis%2===0 && tanggal %2===0


switch (true) {
    case tanggalganjil:
        console.log("JALAN AJA BANG");
        break;
    case tanggalgenap:
        console.log("JALAN TERUS");
        break;
    default:
        console.log("MANTAP KETILANG");
        break;
}
